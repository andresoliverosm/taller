import numpy

nums1 = numpy.array([3,7,4,9,1])
nums2 = numpy.array([[6.3,7.2,1.1],[9.6,5.7,2.4]])
print(f"Datos y tama単o del vector nums1: {nums1[0:4]} y {nums1.size}")
print(f"Datos y tama単o del vector nums2: {nums2}, {len(nums2)}({nums2.size})")

nums1[2] = 100
nums2[0,:] = 83
print(f"Datos y tama単o del vector nums1: {nums1[1:4]} y {nums1.size}")
print(f"Datos y tama単o del vector nums2: {nums2}, {len(nums2)}({nums2.size})")